{
    "name": "casper/attachment-bundle",
    "type": "symfony-bundle",
    "description": "Abstract classes which can be extended to create single attachment entity, or container->attachment relations.",
    "keywords": ["Symfony", "Doctrine", "Entity", "Attachment"],
    "homepage": "https://github.com/yury-maltsev/CasperAttachmentBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yury Maltsev",
            "email": "dev.yury.maltsev@gmail.com"
        }

    ],

    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.1",
        "doctrine/orm": ">=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": ">=2.3",
        "sensio/framework-extra-bundle": ">=2.1"
    },

    "autoload": {
        "psr-0": { "Casper\\CasperAttachmentBundle": "" }
    },

    "archive": {
        "exclude": [ "/Tests" ]
    },

    "target-dir": "Casper/CasperAttachmentBundle"
}
